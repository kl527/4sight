{
  "expo": {
    "name": "foursight",
    "slug": "foursight",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "foursight",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.leed4219.foursight",
      "appleTeamId": "R8ZRDU733W",
      "deploymentTarget": "16.0",
      "associatedDomains": ["applinks:www.kyungsub.com"],
      "infoPlist": {
        "NSBluetoothAlwaysUsageDescription": "foursight uses Bluetooth to connect to your Bangle.js watch and Ray-Ban Meta glasses.",
        "CFBundleURLTypes": [{ "CFBundleTypeRole": "Editor", "CFBundleURLName": "$(PRODUCT_BUNDLE_IDENTIFIER)", "CFBundleURLSchemes": ["foursight"] }],
        "LSApplicationQueriesSchemes": ["fb-viewapp"],
        "UISupportedExternalAccessoryProtocols": ["com.meta.ar.wearable"],
        "UIBackgroundModes": ["bluetooth-central", "bluetooth-peripheral", "external-accessory"],
        "MWDAT": {
          "AppLinkURLScheme": "foursight://",
          "MetaAppID": "901640146132243",
          "ClientToken": "AR|901640146132243|d4ea397962070543613a92b684b52ce0",
          "TeamID": "R8ZRDU733W"
        }
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.leed4219.foursight"
    },
    "androidNavigationBar": {
      "backgroundColor": "#EDECE7",
      "barStyle": "dark-content",
      "enforceContrast": false
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "./plugins/withUseFrameworks",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "resizeMode": "contain",
          "backgroundColor": "#EDECE7",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "react-native-ble-plx"
    ],
    "extra": {
      "apiBaseUrl": "https://foresight-backend.jun-871.workers.dev"
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
