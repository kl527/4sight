{
  "expo": {
    "name": "4sight",
    "slug": "4sight",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "4sight",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.leed4219.x4sight",
      "appleTeamId": "R8ZRDU733W",
      "infoPlist": {
        "NSBluetoothAlwaysUsageDescription": "4sight uses Bluetooth to connect to your Bangle.js watch for health sensor data.",
        "UIBackgroundModes": ["bluetooth-central"]
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.leed4219.x4sight"
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "resizeMode": "contain",
          "backgroundColor": "#EDECE7",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "react-native-ble-plx"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
